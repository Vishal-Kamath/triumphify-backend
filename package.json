{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:app": "npm run build & npm run prod:app",
    "start:admin": "npm run build & npm run prod:admin",
    "start:cdn": "npm run build & npm run prod:cdn",
    "build": "tsc",
    "watch": "tsc --watch",
    "prod:app": "cross-env NODE_ENV=production node ./dist/app/server.js",
    "prod:admin": "cross-env NODE_ENV=production node ./dist/admin/server.js",
    "prod:cdn": "cross-env NODE_ENV=production node ./dist/cdn/server.js",
    "dev:app": "cross-env NODE_ENV=development nodemon ./src/app/server.ts",
    "dev:admin-old": "cross-env NODE_ENV=development nodemon ./src/admin-old/server.ts",
    "dev:admin": "cross-env NODE_ENV=development nodemon ./src/admin/server.ts",
    "dev:cdn": "cross-env NODE_ENV=development nodemon ./src/cdn/server.ts",
    "migration:generate": "drizzle-kit generate:mysql --schema=./src/lib/db/schema.ts",
    "migration:push": "node -r esbuild-register src/lib/db/migrate.ts",
    "migrate": "drizzle-kit generate:mysql --schema=./src/lib/db/schema.ts && node -r esbuild-register src/lib/db/migrate.ts",
    "studio": "drizzle-kit studio --host 127.0.0.1",
    "initSuperAdmin": "ts-node ./src/lib/db/initSuperAdmin.ts",
    "generatePrivilages": "ts-node ./src/lib/db/generatePrivilages.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trycourier/courier": "^6.1.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.2",
    "drizzle-orm": "^0.29.3",
    "envsafe": "^2.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mysql2": "^3.9.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.33.2",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@app": "dist/app",
    "@admin-old": "dist/admin-old",
    "@admin": "dist/admin",
    "@cdn": "dist/cdn",
    "@": "dist"
  }
}
